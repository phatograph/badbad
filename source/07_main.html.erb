<h3 class="text-center">search bar for air-condition</h3>
<form class="form-inline text-center" style="margin: 30px 0;">
  <div class="form-row">
    <div class="form-group">
      <label>Company</label>
      <select class="form-control">
        <option>Option 1</option>
        <option>Option 2</option>
        <option>Option 3</option>
        <option>Option 4</option>
        <option>Option 5</option>
      </select>
    </div>
    <div class="form-group">
      <label>BTU</label>
      <select class="form-control">
        <option>Option 1</option>
        <option>Option 2</option>
        <option>Option 3</option>
        <option>Option 4</option>
        <option>Option 5</option>
      </select>
    </div>
  </div>
  <div class="form-row">
    <div class="form-group">
      <label>Price</label>
      <select class="form-control">
        <option>Option 1</option>
        <option>Option 2</option>
        <option>Option 3</option>
        <option>Option 4</option>
        <option>Option 5</option>
      </select>
    </div>
    <div class="form-group">
      <label>Type</label>
      <label class="checkbox-inline">
        <input type="checkbox" id="inlineCheckbox1" value="option1"> 1
      </label>
      <label class="checkbox-inline">
        <input type="checkbox" id="inlineCheckbox2" value="option2"> 2
      </label>
      <label class="checkbox-inline">
        <input type="checkbox" id="inlineCheckbox3" value="option3"> 3
      </label>
    </div>
  </div>
  <div class="form-row">
    <div class="form-group">
      <label>Function</label>
      <label class="checkbox-inline">
        <input type="checkbox" id="inlineCheckbox1" value="option1"> 1
      </label>
      <label class="checkbox-inline">
        <input type="checkbox" id="inlineCheckbox2" value="option2"> 2
      </label>
      <label class="checkbox-inline">
        <input type="checkbox" id="inlineCheckbox3" value="option3"> 3
      </label>
    </div>
  </div>
</form>
<div class="row">
  <% 5.times do |i| %>
    <div class="col-xs-4">
      <div class="thumbnail">
        <img data-src="holder.js/350x300">
        <div class="caption">
          <h4>Thumbnail label</h4>
          <p><strong>Name:</strong> Name <%= i %></p>
          <p><strong>Model:</strong> Model <%= i %></p>
          <p><strong>Price:</strong> Price <%= i %></p>
          <p><strong>BTU:</strong> BTU <%= i %></p>
        </div>
      </div>
    </div>
  <% end %>
</div>
<div class="text-center">
  <ul class="pagination">
    <li><a href="#">&laquo;</a></li>
    <li class="active"><a href="#">1</a></li>
    <li><a href="#">2</a></li>
    <li><a href="#">3</a></li>
    <li><a href="#">4</a></li>
    <li><a href="#">5</a></li>
    <li><a href="#">&raquo;</a></li>
  </ul>
</div>
